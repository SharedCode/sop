{
  "id": "doctor_pipeline",
  "name": "Dr. Pipeline",
  "description": "I am a pipeline-based medical assistant.",
  "user_prompt": "Patient> ",
  "assistant_name": "AI Doctor",
  "policies": [
    {
      "id": "profanity_filter",
      "type": "profanity",
      "max_strikes": 3
    }
  ],
  "agents": [
    {
      "id": "doctor_core",
      "name": "Dr. Core",
      "description": "I am the core medical knowledge base.",
      "system_prompt": "You are a medical expert. Based on the provided canonical symptoms, diagnose the condition.",
      "storage_path": "data/doctor_core",
      "db_type": "standalone",
      "enable_ingestion_buffer": true,
      "auto_optimize": true,
      "requirements": {
        "vector_store": true,
        "search": false,
        "model_store": false
      },
      "data": []
    }
  ],
  "pipeline": [
    {
      "agent": "profanity_filter"
    },
    {
      "agent": "nurse_local",
      "output_to": "next_step"
    },
    {
      "agent": "doctor_core"
    }
  ]
}
